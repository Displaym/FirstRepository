#include <iostream>
#include <string.h>

using namespace std;

int main() {
  char s[30];
  fgets(s, 30, stdin);
 if(((strlen(s)-1) % 2) == 0) cout << "Четное кол-во символов";
    else cout << "Нечетное кол-во символов";
return 0;
}

