#include <iostream>

using namespace std;

int main () {
  char alphabet[34];
  char letter;
  int index;
  for(letter = 'A', index = 0; letter <= 'Z'; letter++, index++)  alphabet[index] = letter; 
  alphabet[index] = 0;
  cout << "Буквы: " << alphabet;
return 0;
}



#include <iostream>

using namespace std;

int StringToInt(char c[]);

int main () {
  char c[30] = "F";
  cout << StringToInt(c);
return 0;
}

int StringToInt(char c[]) {
  int n, i;
  for (i=1; !(c[i]>='0' && c[i]<='9'); i++)
    if (c[i]== '\0') return 0;
    for (n=0; c[i]>='0' && c[i]<='9'; i++)
      n = n*10 + c[i] - '0';
return n;
}
